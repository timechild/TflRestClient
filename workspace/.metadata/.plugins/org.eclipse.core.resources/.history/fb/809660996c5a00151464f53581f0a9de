package org.disruptiontables.util;

public class ParseData {
	
	public String parseParams(String input){
		
		if(input.equals(""))
			return null;
		
		else
			return input;
	}
	
	public boolean parseStringToInt(String input){
		if(Integer.getInteger(input)==null)
			return true;
	
		else
			return false;
		
	}
	

}
